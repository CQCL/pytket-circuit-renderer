{
  "name": "pytket-circuit-renderer",
  "description": "A Vue 3 component for rendering pytket circuits.",
  "version": "0.5.0",
  "private": false,
  "author": {
    "name": "Tiffany Duneau",
    "email": "tiffany.duneau@cambridgequantum.com"
  },
  "license": "Apache-2.0",
  "repository": "https://github.com/CQCL/pytket-circuit-renderer",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name pytket-circuit-renderer src/main.js",
    "lint": "npx eslint 'src/**/*.vue' 'src/**/*.js' 'cypress/**/*.js' '.storybook/**/*.js' './*.js'",
    "lint-fix": "npx eslint 'src/**/*.vue' 'src/**/*.js' 'cypress/**/*.js' '.storybook/**/*.js' './*.js' --fix",
    "test": "npx cypress run --component",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "main": "./dist/pytket-circuit-renderer.common.js",
  "dependencies": {
    "@cypress/vue": "^3.1.2",
    "@cypress/webpack-dev-server": "^2.0.0",
    "@storybook/testing-vue3": "0.0.2",
    "core-js": "^3.8.3",
    "cypress": "^10.4.0",
    "dom-to-image": "^2.6.0",
    "vue": "^3.2.39"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@storybook/vue3": "^6.5.10",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.22.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.3.0",
    "node-sass": "^7.0.3",
    "sass": "^1.56.0",
    "sass-loader": "^10.3.1",
    "serve": "^13.0.2",
    "vue-loader": "^16.8.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
