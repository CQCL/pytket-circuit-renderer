{
  "name": "pytket-circuit-renderer",
  "description": "A Vue 3 component for rendering pytket circuits.",
  "version": "0.9.0",
  "private": false,
  "author": {
    "name": "Tiffany Duneau",
    "email": "tiffany.duneau@cambridgequantum.com"
  },
  "license": "Apache-2.0",
  "repository": "https://github.com/CQCL/pytket-circuit-renderer",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name pytket-circuit-renderer src/main.js --mode production",
    "lint": "npx eslint 'src/**/*.vue' 'src/**/*.js' 'cypress/**/*.js' '.storybook/**/*.js' './*.js'",
    "lint-fix": "npx eslint 'src/**/*.vue' 'src/**/*.js' 'cypress/**/*.js' '.storybook/**/*.js' './*.js' --fix",
    "test": "npx cypress run --component",
    "cypress": "npx cypress open --component",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "parser": "peggy -o lib/parser.js --format es src/parser.peggy"
  },
  "main": "./dist/pytket-circuit-renderer.common.js",
  "dependencies": {
    "dom-to-image": "^2.6.0",
    "mathjs": "^11.6.0",
    "vue": "^3.2.39"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@cypress/vue": "^3.1.2",
    "@cypress/webpack-dev-server": "^2.0.0",
    "@storybook/addon-actions": "^8.0.4",
    "@storybook/addon-essentials": "^8.0.4",
    "@storybook/addon-interactions": "^8.0.4",
    "@storybook/addon-links": "^8.0.4",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/test": "^8.0.4",
    "@storybook/vue3": "^8.0.4",
    "@storybook/vue3-webpack5": "^8.0.4",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "^5.0.8",
    "babel-loader": "^8.2.5",
    "cypress": "^13.7.1",
    "eslint": "^8.22.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.3.0",
    "node-sass": "^9.0.0",
    "peggy": "^4.0.3",
    "sass": "^1.56.0",
    "sass-loader": "^10.3.1",
    "serve": "^14.2.1",
    "storybook": "^8.0.4",
    "vue-loader": "^16.8.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
